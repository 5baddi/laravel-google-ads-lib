{
    "name": "5baddi/laravel-google-ads-lib",
    "description": "Google Adwords API library for Laravel",
    "keywords": ["google","adwords","ads","laravel", "lib"],
    "license": "MIT",
    "authors": [
        {
            "name": "5BADDI",
            "email": "project@baddi.info"
        }
    ],
    "require": {
        "illuminate/support": "^5.1||^6.0",
        "illuminate/console": "^5.1||^6.0",
        "googleads/googleads-php-lib": ">=37.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^8.0"
    },
    "autoload": {
      "psr-4": {
        "BADDIGroup\\GoogleAds\\": "src/"
      },
      "files": [
          "src/Support/helpers.php"
      ]
    },
    "extra": {
      "laravel": {
        "providers": [
          "BADDIGroup\\GoogleAds\\Providers\\GoogleAdsServiceProvider"
        ],
        "aliases": {
          "GoogleAds": "BADDIGroup\\GoogleAds\\Facades\\GoogleAds"
        }
      }
    },
    "minimum-stability": "dev"
}
